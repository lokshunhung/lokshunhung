{
    "name": "lokshunhung",
    "version": "0.0.1",
    "license": "MIT",
    "main": "dist/build.js",
    "targets": {
        "main": {
            "context": "node",
            "includeNodeModules": false
        }
    },
    "scripts": {
        "start": "parcel serve src/index.html --no-autoinstall",
        "format": "prettier --write \"**/*.{css,html,js,json,jsx,ts,tsx}\"",
        "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
        "build": "parcel build src/build.tsx --no-scope-hoist --no-cache --no-source-maps --no-content-hash && node dist/build.js"
    },
    "dependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@babel/core": "7.14.3",
        "@babel/preset-env": "7.14.4",
        "@babel/preset-react": "7.13.13",
        "@babel/preset-typescript": "7.13.0",
        "@parcel/optimizer-data-url": "2.0.0-beta.3.1",
        "@parcel/transformer-inline-string": "2.0.0-beta.3.1",
        "@parcel/transformer-sass": "2.0.0-beta.3.1",
        "@types/node": "15.12.2",
        "@types/prettier": "2.2.3",
        "@types/react": "17.0.9",
        "@types/react-dom": "17.0.6",
        "@vnus/prettier-config": "0.0.3",
        "eslint": "7.28.0",
        "eslint-config-vnus": "0.0.7",
        "parcel": "2.0.0-beta.3.1",
        "prettier": "2.3.1",
        "sass": "1.34.1",
        "typescript": "4.3.2"
    },
    "prettier": "@vnus/prettier-config",
    "eslintConfig": {
        "ignorePatterns": "dist/**",
        "extends": "vnus/presets/react",
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "rules": {
            "@typescript-eslint/prefer-readonly-parameter-types": "off"
        }
    }
}
