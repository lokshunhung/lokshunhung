{
    "name": "lokshunhung",
    "version": "0.0.1",
    "license": "MIT",
    "main": "dist/build.js",
    "targets": {
        "main": {
            "context": "node",
            "includeNodeModules": false
        }
    },
    "scripts": {
        "start": "parcel serve src/index.html --no-autoinstall",
        "format": "prettier --write \"**/*.{css,html,js,json,jsx,ts,tsx}\"",
        "build": "parcel build src/build.tsx --no-scope-hoist --no-cache --no-source-maps --no-content-hash && node dist/build.js"
    },
    "dependencies": {
        "react": "17.0.1",
        "react-dom": "17.0.1"
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-react": "7.12.10",
        "@babel/preset-typescript": "7.12.7",
        "@types/node": "14.14.20",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/svgo": "1.3.3",
        "@vnus/prettier-config": "0.0.1",
        "parcel": "2.0.0-nightly.524",
        "prettier": "2.2.1",
        "sass": "1.32.4",
        "svgo": "1.3.2",
        "typescript": "4.1.3"
    },
    "prettier": "@vnus/prettier-config"
}
